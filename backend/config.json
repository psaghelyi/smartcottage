{
    "listeners": {
        "*:80": {
            "pass": "applications/fastapi"
        },
        "*:443": {
            "pass": "applications/fastapi",
            "tls": {
                "certificate": "bundle"
            }
        }
    },

    "applications": {
        "fastapi": {
            "type": "python 3.10",
            "processes": 4,
            "path": "/fastapi",
            "home": "/usr/local",
            "module": "asgi",
            "callable": "app"
        }
    }
}
